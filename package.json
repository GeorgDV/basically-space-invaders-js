{
  "name": "some-game",
  "version": "1.0.0",
  "description": "This is a project including working with something to make something cool.",
  "main": "index.js",
  "dependencies": {
    "parcel-bundler": "^1.12.5"
  },
  "devDependencies": {
    "browserify-fs": "^1.0.0",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "sass": "^1.32.8",
    "startaudiocontext": "^1.2.1",
    "tone": "^14.7.77"
  },
  "scripts": {
    "clean": "rm -rf dist/",
    "dev": "parcel serve ./public/index.html",
    "redev": "rm -rf dist/ && parcel serve ./public/index.html",
    "build": "parcel build ./public/index.html --no-content-hash --no-source-maps --no-cache --no-minify",
    "deploy": "parcel build ./public/index.html --no-content-hash --no-source-maps --no-cache --no-minify && surge dist/ --domain https://basically-space-invaders.surge.sh",
    "redeploy": "rm -rf dist/ && parcel build ./public/index.html --no-content-hash --no-source-maps --no-cache --no-minify && surge dist/ --domain https://basically-space-invaders.surge.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeorgDV/some-game.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GeorgDV/some-game/issues"
  },
  "homepage": "https://github.com/GeorgDV/some-game#readme",
  "staticFiles": {
    "staticPath": ["public", "src", "static"],
    "watcherGlob": "**"
  }
}
